          },
          "quantity": {
            "type": "number",
            "format": "double"
          },
          "averagePrice": {
            "type": "number",
            "format": "double",
            "minimum": 0
          },
          "commission": {
            "type": "number",
            "format": "double",
            "minimum": 0
          },
          "purchasedDate": {
            "type": "object"
          },
          "instrument": {
            "type": "object",
            "properties": {
              "symbol": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "assetType": {
                "type": "string",
                "enum": [
                  "EQUITY",
                  "OPTION",
                  "MUTUAL_FUND",
                  "FIXED_INCOME",
                  "INDEX"
                ]
              }
            }
          },
          "status": {
            "type": "string",
            "enum": [
              "UNCHANGED",
              "CREATED",
              "UPDATED",
              "DELETED"
            ]
